{
  "name": "JackettPlugin",
  "title": "Jackett搜索",
  "author": "jason",
  "description": "通过Jackett API扩展资源搜索能力，支持多个资源站点搜索。",
  "version": "1.08",
  "requirements": [],
  "state": true,
  "pluginPath": "plugins.v2/jackett",
  "moduleName": "JackettPlugin",
  "loadOrder": 21,
  "githubUrl": "",
  "helpUrl": "",
  "icon": "https://raw.githubusercontent.com/Jackett/Jackett/master/src/Jackett.Common/Content/favicon.ico",
  "color": "#2196F3",
  "dependencies": {
    "aiohttp": ">=3.8.0",
    "pydantic": ">=2.0.0"
  },
  "configs": {
    "host": {
      "name": "服务器地址",
      "type": "string",
      "default": "http://localhost:9117",
      "description": "Jackett服务器地址"
    },
    "api_key": {
      "name": "API密钥",
      "type": "string",
      "default": "",
      "description": "Jackett API密钥"
    },
    "indexers": {
      "name": "搜索源列表",
      "type": "array",
      "default": [],
      "description": "指定搜索源列表"
    },
    "proxy": {
      "name": "代理服务器",
      "type": "string",
      "default": "",
      "description": "代理服务器地址"
    },
    "verify_ssl": {
      "name": "验证SSL证书",
      "type": "boolean",
      "default": false,
      "description": "是否验证SSL证书"
    },
    "timeout": {
      "name": "超时时间",
      "type": "integer",
      "default": 30,
      "description": "请求超时时间(秒)"
    }
  }
} 